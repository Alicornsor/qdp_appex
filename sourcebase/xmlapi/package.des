<?xml version="1.0" encoding="gb2312"?>

<packageDefine name="xmlapi" version="1" type="cpp" includedir="." platform="all">
	<parameter>
		<useParameter name="systemConfigEnv"/>
		<useParameter name="deployConfigEnv"/>
		<useParameter name="mergeConfigEnv"/>
	</parameter>
	<depend>
		<package name="platform" version="1"/>
		<package name="base" version="1"/>
		<package name="probelogger" version="1"/>
		<package name="utility" version="1"/>
		<package name="datastruct" version="1"/>
		<package name="xml" version="1"/>
	</depend>
	<cppfile>
		<file name="CConfigXML.cpp"/>
		<file name="CSystemConfig.cpp"/>
		<file name="CDeployConfig.cpp"/>
		<file name="CMergeConfig.cpp"/>
	</cppfile>
	<headfile>
		<file name="CConfigXML.h"/>
		<file name="CSystemConfig.h"/>
		<file name="CDeployConfig.h"/>
		<file name="CDeployConfig.h.tpl"/>
		<file name="CDeployConfig.cpp.tpl"/>
		<file name="CSystemConfig.h.tpl"/>
		<file name="CSystemConfig.cpp.tpl"/>
		<file name="CMergeConfig.h"/>
		<file name="CMergeConfig.h.tpl"/>
