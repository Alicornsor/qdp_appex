			<target type="file" value="CDeployConfig.cpp"/>
			<template type="file" value="CDeployConfig.cpp.tpl"/>
			<env type="parameter" value="deployConfigEnv"/>
		</pump>
		<pump>
			<target type="file" value="CMergeConfig.h"/>
			<template type="file" value="CMergeConfig.h.tpl"/>
			<env type="parameter" value="mergeConfigEnv"/>
